{
  "name": "tracerdebug",
  "description": "A powerful and customizable logging library integrated with debug for node.js. support color console with timestamp, line number, method name, file name and call stack. you can set transport to file, stream, database(ex: mongodb and clouddb, simpledb). keywords: log, logger, trace ",
  "homepage": "http://github.com/yknx4/tracer",
  "repository": {
    "type": "git",
    "url": "git://github.com/yknx4/tracer.git"
  },
  "version": "0.8.9",
  "author": "Ale Figueroa <me@ale.world>",
  "license": "MIT",
  "dependencies": {
    "colors": "1.1.2",
    "dateformat": "2.0.0",
    "debug": "^2.6.8",
    "lodash": "^4.17.4",
    "tinytim": "0.1.1"
  },
  "devDependencies": {
    "cli-color": "^1.1.0",
    "eslint": "^2.13.1",
    "expresso": "^0.9.2",
    "istanbul": "^0.4.5",
    "mongoskin": "0.3.0"
  },
  "main": "./lib/index",
  "keywords": [
    "log",
    "logger",
    "trace",
    "debug"
  ],
  "scripts": {
    "test": "expresso test/*",
    "lint": "eslint .",
    "posttest": "npm run lint",
    "test:coverage": "istanbul cover expresso test/*.js",
    "posttest:coverage": "npm run lint"
  },
  "engines": {
    "node": ">= 0.10.0"
  }
}
